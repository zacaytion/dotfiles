<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>KeepAlive</key>
	<dict>
		<key>Crashed</key>
		<true/>
		<key>SuccessfulExit</key>
		<false/>
	</dict>
	<key>Label</key>
	<string>local.emacs.daemon</string>
	<key>LimitLoadToSessionType</key>
	<array>
		<string>Aqua</string>
		<string>StandardIO</string>
		<string>Background</string>
		<string>LoginWindow</string>
	</array>
	<key>Nice</key>
	<integer>-20</integer>
	<key>ProcessType</key>
	<string>Interactive</string>
	<key>Program</key>
	<string>/opt/homebrew/bin/zsh</string>
	<key>ProgramArguments</key>
	<array>
		<string>/opt/homebrew/bin/zsh</string>
		<string>-c</string>
		<string>emacs --fg-daemon</string>
	</array>
	<key>RunAtLoad</key>
	<true/>
	<key>StandardErrorPath</key>
	<string>/tmp/local.emacs.daemon.stderr</string>
	<key>StandardOutPath</key>
	<string>/tmp/local.emacs.daemon.stdout</string>
</dict>
</plist>
